  object qryDetalheCusto: TUniQuery
    Connection = DmLogin.Conexao
    SQL.Strings = (
      'SELECT PCEMBALAGEM.CODAUXILIAR,'
      '       PCPRODUT.CODPROD,'
      '       PCPRODUT.DESCRICAO,'
      
        '       (PKG_I9UTILITARIO.BUSCACUSTO (PCEMBALAGEM.CODFILIAL, PCEM' +
        'BALAGEM.CODPROD) * NVL(PCEMBALAGEM.QTUNIT,1)) CUSTOFIN,'
      '       NVL (PCTRIBUT.PERDESCCUSTO, 0) PERCDESCCUSTO,'
      
        '       ((ROUND (PKG_I9UTILITARIO.BUSCACUSTO (PCEMBALAGEM.CODFILI' +
        'AL, PCEMBALAGEM.CODPROD) * (NVL (PCTRIBUT.PERDESCCUSTO, 0) / 100' +
        '), 2))  * NVL(PCEMBALAGEM.QTUNIT,1)) VLDESCCUSTO,'
      
        '       PKG_I9UTILITARIO.BUSCACUSTO (PCEMBALAGEM.CODFILIAL, PCEMB' +
        'ALAGEM.CODPROD) - ROUND (PKG_I9UTILITARIO.BUSCACUSTO (PCEMBALAGE' +
        'M.CODFILIAL, PCEMBALAGEM.CODPROD) * (NVL (PCTRIBUT.PERDESCCUSTO,' +
        ' 0) / 100), 2) VLCUSTOFINAL,'
      '       NVL (PCPRODUT.PCOMREP1, 0) PERCCOMISSAO,'
      '       ROUND ('
      
        '           PKG_I9UTILITARIO.CALCULAR_SUGESTAO_COMPLETA (PCEMBALA' +
        'GEM.CODFILIAL, PCEMBALAGEM.CODAUXILIAR, '#39'VAREJO'#39') * (NVL (PCPROD' +
        'UT.PCOMREP1, 0) / 100),'
      '           2)'
      '           VLCOMISSAO,'
      '       NVL (PCTRIBUT.CODICMTABPF, 0) PERCIMPOSTOESTADUAL,'
      '       ROUND ('
      
        '           PKG_I9UTILITARIO.CALCULAR_SUGESTAO_COMPLETA (PCEMBALA' +
        'GEM.CODFILIAL, PCEMBALAGEM.CODAUXILIAR, '#39'VAREJO'#39')'
      '           * (NVL (PCTRIBUT.CODICMTABPF, 0) / 100),'
      '           2)'
      '           VLIMPOSTOESTADUAL,'
      '       NVL (PCCONSUM.TXVENDA, 0) PERCIMPOSTOFEDERAL,'
      '       ROUND ('
      
        '           PKG_I9UTILITARIO.CALCULAR_SUGESTAO_COMPLETA (PCEMBALA' +
        'GEM.CODFILIAL, PCEMBALAGEM.CODAUXILIAR, '#39'VAREJO'#39') * (NVL (PCCONS' +
        'UM.TXVENDA, 0) / 100),'
      '           2)'
      '           VLIMPOSTOFEDERAL,'
      
        '       PKG_I9UTILITARIO.BUSCA_MARGEM_PRODUTO (PCEMBALAGEM.CODFIL' +
        'IAL, PCEMBALAGEM.CODPROD, '#39'VAREJO'#39') PERCMARGEM,'
      '       ROUND ('
      
        '           PKG_I9UTILITARIO.CALCULAR_SUGESTAO_COMPLETA (PCEMBALA' +
        'GEM.CODFILIAL, PCEMBALAGEM.CODAUXILIAR, '#39'VAREJO'#39')'
      
        '           * (PKG_I9UTILITARIO.BUSCA_MARGEM_PRODUTO (PCEMBALAGEM' +
        '.CODFILIAL, PCEMBALAGEM.CODPROD, '#39'VAREJO'#39') / 100),'
      '           2)'
      '           VLMARGEM,'
      
        '       PKG_I9UTILITARIO.CALCULAR_SUGESTAO_COMPLETA (PCEMBALAGEM.' +
        'CODFILIAL, PCEMBALAGEM.CODAUXILIAR, '#39'VAREJO'#39') VLSUGESTAO'
      'FROM PCEMBALAGEM,'
      '     PCPRODUT,'
      '     PCTABTRIB,'
      '     PCTRIBUT,'
      '     PCFILIAL,'
      '     PCCONSUM'
      'WHERE     PCEMBALAGEM.CODPROD = PCPRODUT.CODPROD'
      '      AND PCTABTRIB.CODPROD = PCPRODUT.CODPROD'
      '      AND PCFILIAL.CODIGO = PCEMBALAGEM.CODFILIAL'
      '      AND PCTABTRIB.CODFILIALNF = PCFILIAL.CODIGO'
      '      AND PCTRIBUT.CODST = PCTABTRIB.CODST'
      '      AND PCTABTRIB.UFDESTINO = PCFILIAL.UF'
      '      AND PCEMBALAGEM.CODFILIAL = :CODFILIAL'
      '      AND PCEMBALAGEM.CODAUXILIAR = :CODAUXILIAR')
    Left = 40
    Top = 92
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'codfilial'
        Value = nil
      end
      item
        DataType = ftUnknown
        Name = 'CODAUXILIAR'
        Value = nil
      end>
